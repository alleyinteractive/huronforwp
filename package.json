{
  "name": "huronforwp",
  "version": "1.0.0",
  "description": "An example of a WordPress theme that uses Huron for prototying.",
  "main": "index.js",
  "engines": {
    "node": "7",
    "npm": "5"
  },
  "scripts": {
    "build": "webpack --config src/config/webpack.config.js -p --env.production",
    "dev": "webpack-dev-server --config src/config/webpack.config.js --env.dev",
    "huron": "huron -w src/config/webpack.config.js -c src/config/huron.config.js --env.huron",
    "huron-build": "huron -w src/config/webpack.config.js -c src/config/huron.config.js -p --env.huron"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pattiereaves/huronforwp.git"
  },
  "keywords": [
    "huron",
    "prototyping"
  ],
  "author": "Alley Interactive",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alleyinteractive/huronforwp/issues"
  },
  "homepage": "https://github.com/alleyinteractive/huronforwp#readme",
  "devDependencies": {
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.1.2",
    "huron": "^2.2.0",
    "node-sass": "^4.5.3",
    "sass-loader": "6.0.6",
    "webpack": "^2.7.0"
  }
}
